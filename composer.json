{
    "name": "tylercd100/monolog-mailgun",
    "description": "A Monolog Handler for SMS messaging services such as Plivo and Twilio.",
    "keywords": [
        "monolog",
        "monolog handler",
        "mailgun",
        "tylercd100"
    ],
    "homepage": "https://github.com/tylercd100/monolog-mailgun",
    "license": "MIT",
    "autoload":{
        "psr-4":{
            "Tylercd100\\Monolog\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tylercd100\\Monolog\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "monolog/monolog": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2.3|^9.0"
    },
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        }
    ]
}
